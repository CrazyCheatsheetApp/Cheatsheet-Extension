{
	"name": "cheatsheets",
	"displayName": "Cheatsheets",
	"description": "app to keep notes within vs code for future reference",
	"version": "0.1.0",
	"repository": {
		"url": "https://github.com/CrazyCheatsheetApp"
	},
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cheatSheet.start",
		"onView:cheatsheets"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cheatSheet.start",
				"title": "Cheatsheets",
				"category": "Cheat Sheet"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "cheatSheet.start",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"publisher": "CrazyCheatsheets"
}